<script type="text/javascript">
    @section Vue
    {
    import {
        SignUp, GetListOfCity, GetDistrictsByCity,
        GetListOfCompetitionItems
    } from '@(configuration.GetSection("BaseUrl").Value)/StaticFiles/Services/ApiServices.js'
    const app = Vue.createApp({
        data: function () {
            return {
                selectedChecked: null,
                cityListInfo: null,
                districtsListInfo: null,
                cityList: null,
                districtsList: null,
                submitData: {},
                competitionList: null,
                checkbox: [],
                agreeOne: false,
                agreeTwo: false,
                agreeThree: false,
                checked: "",
            }
        },
        created: function () {
            this.GetCity();
            this.GetCompetitionItems();
        },
        methods: {
            GetOtherRoute: function () {
                try {
                    window.location.href = '@Url.Action("Index", "ArtRoute")';
                }
                catch (exception) {
                    console.log(exception);
                }
            },
            submitForm: async function () {
                try {
                    const response = await SignUp(this.submitData).then(response => response);
                    const status = response.status;
                    if (status === 200) {
                        const data = response.data;
                        const code = data.RtnCode;
                        switch (code) {
                            case 200:
                                window.location.href = '@Url.Action("AcademicExperience", "ArtRoute")?uid=' + data.RtnData;
                                break;
                            default:
                                break;
                        }
                    }

                }
                catch (exception) {
                    console.log(exception);
                }
                console.log(this.submitData);
            },
            GetSelectCompetitionItems: function () {
                let obj = document.getElementById('Cid');
                this.submitData.Cid = obj.value;
            },
            GetCompetitionItems: async function () {
                try {
                    const response = await GetListOfCompetitionItems().then(response => response);
                    const status = response.status;
                    if (status === 200) {
                        this.competitionList = response.data;
                    }
                }
                catch (exception) {
                    console.log(exception);
                }
            },
            GetCity: async function (isTF) {
                try {
                    const response = await GetListOfCity().then(response => response);
                    const status = response.status;
                    if (status === 200) {
                        const data = response.data;
                        const code = data.RtnCode;
                        switch (code) {
                            case 200:
                                this.cityList = data.RtnData;
                                this.cityListInfo = data.RtnData;
                                break;
                            default:
                                console.log(data.RtnData)
                                break;
                        }
                    }
                }
                catch (exception) {
                    console.log(exception);
                }
            },
            GetDistrictsData: function (isTF) {
                if (isTF) {
                    let obj = document.getElementById('PermanentDistrict');
                    this.submitData.PermanentDistrict = obj.value;
                }
                else {
                    let obj = document.getElementById('MailingDistrict');
                    this.submitData.MailingDistrict = obj.value;
                }
            },
            GetDistricts: async function (isTF) {
                try {
                    let data = 0;
                    if (isTF) {
                        let obj = document.getElementById('PermanentCity');
                        this.submitData.PermanentCity = obj.value;
                        data = this.submitData.PermanentCity;
                    }
                    else {
                        let obj = document.getElementById('MailingCity');
                        this.submitData.MailingCity = obj.value;
                        data = this.submitData.MailingCity
                    }
                    const response = await GetDistrictsByCity(data).then(response => response);
                    const status = response.status;
                    if (status === 200) {
                        const data = response.data;
                        const code = data.RtnCode;
                        switch (code) {
                            case 200:
                                if (isTF)
                                    this.districtsList = data.RtnData;
                                else
                                    this.districtsListInfo = data.RtnData;
                                break;
                            default:
                                console.log(data.RtnData)
                                break;
                        }
                    }
                }
                catch (exception) {
                    console.log(exception);
                }
            },
            IsRequired: function (value) {
                if (!value) {
                    return '請務必填寫';
                }
                return true;
            },
            IsChecked: function (value) {
                if (!value) {
                    return '請務必仔細詳閱';
                }
                return true;
            },
            Checked: function () {
                if (this.agreeOne & this.agreeTwo & this.agreeThree)
                    this.checked = "閱讀";
                else
                    this.checked = "";
            },
            IsChinese: function (value) {
                const regexs = /^[\u4e00-\u9fa5]*$/;
                const result = regexs.test(value);
                return result ? true : '請輸入中文姓名'
            },
            IsEnglish: function (value) {
                if (value == null) {
                    return '請輸入英文姓名';
                }
                const regexs = /^(?=.*[a-zA-Z]).{1,50}$/;
                const result = regexs.test(value);
                return result ? true : '請輸入英文姓名'
            },
            IsID: function (value) {
                const regexs = /^[A-Z]{1}[1-2]{1}[0-9]{8}$/;
                const result = regexs.test(value);
                return result ? true : '請輸入身分證字號'
            },
            IsBirth: function (value) {
                const regexs = /^[1-9]\d{3}-\d{2}-\d{2}$/;
                const result = regexs.test(value);
                return result ? true : '請輸入正確的生日格式'
            },
            IsPhone: function (value) {
                const regexs = /^09[0-9]{8}$/;
                const result = regexs.test(value);
                return result ? true : '請輸入正確的電話格式'
            },
            IsEmail: function (value) {
                if (value == undefined || value == "" || value == null) {
                    return true;
                }
                const regexs = /^\w+((-\w+)|(\.\w+))*\@@[A-Za-z0-9]+((\.|-)[A-Za-z0-9]+)*\.[A-Za-z]+$/;
                const result = regexs.test(value);
                return result ? true : '請輸入正確的Email格式'
            }

        }
    })


    }
</script>
